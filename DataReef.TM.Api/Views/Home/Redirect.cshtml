@model string
@{
     Layout = "~/Views/Shared/_Layout-Legion.cshtml";
   // Layout = null;
    ViewBag.Title = "Ignite - User Registration";
}

@*<script src="~/Scripts/browser-deeplink.js"></script>
<script src="~/Scripts/browser-deeplink.min.js"></script>*@
@section scripts
{

    <script type="text/javascript">
        try {
            document.addEventListener('DOMContentLoaded', function () {
                try {
                    if (validations.isiPad || validations.isiPhone) {
                        var strlink = '@Html.Raw(Model)';
                        if (strlink.indexOf('notes') != -1) {
                            deeplink.open(strlink);
                        }
                        else {
                            location.href = strlink;
                        }
                    } else {
                        var queryString = window.location.href.slice(window.location.href.indexOf('?') + 1);
                    }
                } catch (e) { console.log(e)}
            });
        } catch (e) { }
    </script>
}